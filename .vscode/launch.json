{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Run index.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/index.js",
            "runtimeExecutable": "/usr/local/bin/node"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Run test.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/test.js",
            "runtimeExecutable": "/usr/local/bin/node"
        }
    ]
}